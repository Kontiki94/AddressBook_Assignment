<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AssignmentAddressBookMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AssignmentAddressBookMaui"
    xmlns:pages="clr-namespace:AssignmentAddressBookMaui.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Assignment Address Book Maui"
    >

    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
    <ShellContent Title="Add Contact" ContentTemplate="{DataTemplate pages:AddContactPage}" Route="AddContactPage" />
    <ShellContent Title="Address Book List" ContentTemplate="{DataTemplate pages:AddressBookListPage}" Route="AddressBookListPage" />
    <ShellContent Title="Update Contact" ContentTemplate="{DataTemplate pages:UpdateContactPage}" Route="UpdateContactPage" />

</Shell>
